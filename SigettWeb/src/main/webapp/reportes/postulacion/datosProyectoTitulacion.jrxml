	<queryString>
		<![CDATA[select pr.tema_actual, p.nombres as "nombres_Egdo",p.apellidos as "apellidos_Egdo",p.numero_identificacion as "cedula_Egdo",
c.nombre as "carrera",c.sigla as "carrera_sigla",area.sigla as "area_sigla",area.nombre as "area", p1.nombres as "nombres_coordinador", p1.apellidos as "apellidos_coordinador",p1.numero_identificacion as "cedula_coordinador" 
from autor_proyecto ap inner join aspirante a on ap.aspirante_id=a.id
inner join estudiante_carrera ec on ec.id=a.id
inner join estudiante e on ec.estudiante_id=e.id
inner join persona p on e.id=p.id
inner join proyecto pr on ap.proyecto_id=pr.id
inner join proyecto_carrera_oferta pco on pco.proyecto_id=pr.id
inner join carrera c on pco.carrera_id=c.id
inner join area area on c.area_id=area.id 
inner join periodo_coordinacion pcoor on pcoor.carrera_id=c.id
inner join coordinador_periodo cp on cp.periodo_id=pcoor.id
inner join coordinador coor on cp.coordinador_id=coor.id
inner join persona p1 on coor.id=p1.id
where ap.id=17 and cp.es_vigente=true;]]>
	</queryString>
